FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftWall
    {
        type            alphaContactAngle;
        thetaProperties ( ( water air ) 90 0 0 0 );
        value           uniform 0;
    }
    rightWall
    {
        type            alphaContactAngle;
        thetaProperties ( ( water air ) 90 0 0 0 );
        value           uniform 0;
    }
    lowerWall
    {
        type            alphaContactAngle;
        thetaProperties ( ( water air ) 90 0 0 0 );
        value           uniform 0;
    }
    bottominlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 0;
    }
    bottomoutlet
    {
        type            alphaContactAngle;
        thetaProperties ( ( water air ) 90 0 0 0 );
        value           uniform 0;
    }
    topinlet
    {
        type            alphaContactAngle;
        thetaProperties ( ( water air ) 90 0 0 0 );
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


